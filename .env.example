# Environment variables for Admin/Subscription Bot
# Copy this file to .env and fill in your actual values

# Admin Bot Configuration (DIFFERENT TOKEN!)
ADMIN_BOT_TOKEN=your_admin_bot_token_here
ADMIN_BOT_USERNAME=subscriptionOSINTbot

# Database Configuration (Supabase)
# For local testing with Supabase - if not set, will fallback to SQLite
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Admin Access
ADMIN_USER_ID=5682019164

# Database Access (uses main bot token for shared database)
TELEGRAM_BOT_TOKEN=your_main_bot_token_here
BOT_USERNAME=reosintbot

# Logging
LOG_LEVEL=INFO